server {
    listen {{ http_port }};
    server_name {{ http_host }} www.{{ http_host }};
    root /var/www/{{ http_host }};
    index index.html index.htm;

    error_log /var/log/nginx/error.log;
    access_log /var/log/nginx/access.log;

    location / {
        try_files $uri $uri/ =404;
    }
}
